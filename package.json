    "@types/jest": "^26.0.20",
    "babel-plugin-macros": "^3.0.1",
    "classnames": "^2.2.6",
    "design-system-utils": "^1.5.0",
    "emotion": "^11.0.0",
    "param-case": "^3.0.4",
    "postcss": "^8.2.6",
    "prop-types": "^15.7.2",
    "tailwindcss": "^2.0.3",
    "twin.macro": "^2.3.0",
    "twind": "^0.15.10",
    "xwind": "^0.8.0"
  },
