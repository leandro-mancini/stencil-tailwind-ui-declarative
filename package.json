    "autoprefixer": "^10.2.4",
    "lodash": "^4.17.21",
    "stencil-tailwind": "0.0.6",
    "styled-components": "^5.2.1",
    "styled-system": "^5.1.5",
    "tailwind-styled-components": "^1.0.7"
